{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "first-thingy",
  "title": "first-thingy",
  "description": "",
  "icon": "command-icon.png",
  "author": "shezcode",
  "license": "MIT",
  "commands": [
    {
      "name": "playpause",
      "title": "Play/Pause",
      "description": "Toggle between play and pause",
      "mode": "no-view"
    },
    {
      "name": "shuffle",
      "title": "Shuffle",
      "description": "Toggle shuffle play",
      "mode": "no-view"
    },
    {
      "name": "nextTrack",
      "title": "Next track",
      "description": "Play next track",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.2",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
